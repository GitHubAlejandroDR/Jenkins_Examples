pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building app Alejandro Domínguez Recio'
            }
        }
        stage('Developing') {
            steps {
                echo 'Developing app Alejandro Domínguez Recio'
            }
        }
        stage('Sleeping') {
            steps {
                sleep 30
                echo 'Sleeping'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing app Alejandro Domínguez Recio'
            }
        }
    }
}